<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="_background" fitHeight="1000.0" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true" />
      <VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="1000.0" prefWidth="1000.0">
         <children>
            <HBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="450.0" prefWidth="1000.0">
               <children>
                  <VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="450.0" prefWidth="332.0" />
                  <VBox alignment="BOTTOM_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="450.0" prefWidth="334.0">
                     <children>
                        <ImageView fx:id="_logo" fitHeight="334.0" fitWidth="334.0" pickOnBounds="true" preserveRatio="true" />
                        <Label text="SHIFUMI">
                           <font>
                              <Font size="66.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
                  <VBox alignment="TOP_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="450.0" prefWidth="332.0" spacing="10.0">
                     <children>
                        <Button contentDisplay="RIGHT" mnemonicParsing="false" onAction="#Option" text="OPTION">
                           <graphic>
                              <ImageView fx:id="_option" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
                           </graphic>
                           <font>
                              <Font size="27.0" />
                           </font>
                        </Button>
                        <Button contentDisplay="RIGHT" mnemonicParsing="false" onAction="#Contact" text="CONTACT">
                           <graphic>
                              <ImageView fx:id="_contact" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
                           </graphic>
                           <font>
                              <Font size="27.0" />
                           </font>
                        </Button>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
            <Label fx:id="_status" text="VICTOIRE" textFill="#e8a200">
               <font>
                  <Font size="73.0" />
               </font>
            </Label>
            <Label text="Voulez-vous continuer ?">
               <font>
                  <Font size="40.0" />
               </font>
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="100.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#Oui" text="Oui">
                     <font>
                        <Font size="32.0" />
                     </font>
                     <graphic>
                        <ImageView fx:id="_oui" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
                     </graphic>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#Non" text="Non">
                     <font>
                        <Font size="32.0" />
                     </font>
                     <graphic>
                        <ImageView fx:id="_non" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
                     </graphic>
                  </Button>
               </children>
            </HBox>
            <HBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="278.0" prefWidth="1000.0">
               <children>
                  <StackPane alignment="BOTTOM_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="278.0" prefWidth="500.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#Rules" text="REGLEMENT">
                           <font>
                              <Font size="27.0" />
                           </font>
                           <graphic>
                              <ImageView fx:id="_rule" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
                           </graphic>
                        </Button>
                     </children>
                  </StackPane>
                  <StackPane alignment="BOTTOM_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="278.0" prefWidth="500.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#Leave" text="QUITTER">
                           <graphic>
                              <ImageView fx:id="_leave" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
                           </graphic>
                           <font>
                              <Font size="27.0" />
                           </font>
                        </Button>
                     </children>
                  </StackPane>
               </children>
               <padding>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
